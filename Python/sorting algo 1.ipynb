    "from array import *\n",
    "from random import randint\n",
    "\n",
    "def display(a,n):\n",
    "    for i in range(0,n):\n",
    "        print(a[i], end =\" \")\n",
    "        \n",
    "def partition(a,low,high):\n",
    "    i=low-1\n",
    "    pivot=a[high]\n",
    "    for j in range(low, high):\n",
    "        if (a[j]<=pivot):\n",
    "            i+=1\n",
    "            a[i],a[j]=a[j],a[i]\n",
    "    a[i+1],a[high]=a[high],a[i+1]\n",
    "    return (i+1)\n",
    "    \n",
    "def QuickSort(a,low,high):\n",
    "    if (low<high):\n",
    "        p1=partition(a, low,high)\n",
    "        QuickSort(a, low, p1-1)\n",
    "        QuickSort(a,  p1+1, high)\n",
    "    return a\n",
    "\n",
    "a=array(\"i\",[])\n",
    "i=0\n",
    "while(i<10):\n",
    "    a.insert(i,randint(0,20))\n",
    "    i+=1\n",
    "display(a,10)\n",
    "print()\n",
    "a=QuickSort(a,0,9)\n",
    "print(\"Array after Quick Sort \", display(a,len(a))\n"
